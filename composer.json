{
	"name": "infoweb-internet-solutions/yii2-cms",
	"description": "CMS module for Yii2",
	"keywords": ["yii2", "yii2-cms", "cms", "infoweb"],
	"type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/infoweb-internet-solutions/yii2-cms/issues?state=open",
        "source": "https://github.com/infoweb-internet-solutions/yii2-cms"
    },
    "authors": [
        {
            "name": "Fabio Maggio",
            "email": "fabio@infoweb.be",
            "homepage": "https://github.com/fabiomaggio"
        },
        {
            "name": "Ruben Heymans",
            "email": "ruben@infoweb.be",
            "homepage": "http://www.infoweb.be/"
        }
    ],
	"require": {
        "yiisoft/yii2": "@stable",
        "infoweb-internet-solutions/yii2-cms-settings": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-user": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-pages": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-partials": "1.1.*",
        "infoweb-internet-solutions/yii2-cms-seo": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-menu": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-alias": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-analytics": "1.0.*",
        "infoweb-internet-solutions/yii2-cms-email": "1.0.*",
        "infoweb-internet-solutions/yii2-sortable-behavior": "1.0.*",
        "mdmsoft/yii2-admin": "1.0.3",
        "kartik-v/yii2-helpers": "1.2.0",
        "kartik-v/yii2-widgets": "3.4.0",
        "kartik-v/yii2-grid": "2.9.0",
        "kartik-v/bootstrap-fileinput": "4.1.4",
        "kartik-v/yii2-icons": "1.3.0",
        "kartik-v/yii2-datecontrol": "1.9.0",
        "2amigos/yii2-translateable-behavior" : "0.1.2",
        "costa-rico/yii2-images": "1.0.4",
        "mihaildev/yii2-ckeditor": "dev-infoweb as 1.0.1",
        "zelenin/yii2-i18n-module": "dev-infoweb as 0.1.5",
        "bower-asset/bootbox": "@dev",
        "bower-asset/fastclick": "@stable",
        "bower-asset/cookies-js": "@stable",
        "bower-asset/perfect-scrollbar": "@stable",
        "bower-asset/underscore": "@stable"
